<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<AnchorPane
        prefHeight="400.0"
        prefWidth="600.0"
        xmlns="http://javafx.com/javafx/8.0.111"
        xmlns:fx="http://javafx.com/fxml/1"
        fx:controller="library.fx.MainWindow">
    <GridPane alignment="CENTER" prefHeight="400.0" prefWidth="600.0">
        <columnConstraints>
            <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" valignment="CENTER" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" valignment="CENTER" vgrow="SOMETIMES"/>
        </rowConstraints>
        <padding>
            <Insets bottom="5.0"/>
        </padding>
        <Button mnemonicParsing="false" prefHeight="120.0" prefWidth="249.0" text="Patrons" GridPane.rowIndex="2"/>
        <Button mnemonicParsing="false" prefHeight="120.0" prefWidth="249.0" text="Patrons" GridPane.rowIndex="1">
            <padding>
                <Insets bottom="5.0"/>
            </padding>
        </Button>
        <Button mnemonicParsing="false" prefHeight="120.0" prefWidth="249.0" text="Books" GridPane.columnIndex="1"
                GridPane.rowIndex="1"/>
        <Button mnemonicParsing="false" prefHeight="120.0" prefWidth="249.0" text="Reports" GridPane.columnIndex="1"
                GridPane.rowIndex="2"/>
        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Library Management System">
            <font>
                <Font name="System Bold" size="19.0"/>
            </font>
        </Text>
    </GridPane>
</AnchorPane>
